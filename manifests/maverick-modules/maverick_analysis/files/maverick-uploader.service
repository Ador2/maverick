[Unit]
Description=Maverick Log Uploader
After=network-online.target
Requires=network-online.target

[Service]
TimeoutStartSec=0
ExecStart=/srv/maverick/software/python/bin/python3 /srv/maverick/software/maverick-web-legacy/tornado/file_upload.py
User=mav
TimeoutStartSec=20

[Install]
WantedBy=multi-user.target

