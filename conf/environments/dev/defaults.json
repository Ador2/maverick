// These are defaults for the dev environment.
{
	// Turn SITL services on for dev mode
	"maverick_dev::apsitl_apdev::sitl_active": true, // Whether SITL is active or not
	"maverick_dev::apsitl_apdev::mavlink_active": true, // Whether mavlink proxy is active or not
	"maverick_dev::apsitl_apdev::rosmaster_active": true, // Whether ROS master for sitl is active or not
	"maverick_dev::apsitl_apdev::mavros_active": true, // Whether Mavros for sitl is active or not - requires sitl rosmaster to be active
	"maverick_dev::apsitl_apdev::api_active": false,

	// Turn FC services on for flight mode
	"maverick_fc::mavlink_active": false,
	"maverick_fc::rosmaster_active": false,
	"maverick_fc::mavros_active": false,
	"maverick_fc::api_active": false,
	
	"classes": [
		"base",
		"maverick_security",
	    "maverick_web",
		"maverick_mavlink",
		"maverick_vision",
		"maverick_fc",
		"maverick_ros",
		"maverick_dev",
	    "maverick_intelligence",
	    "maverick_analysis"
	]
	
}
